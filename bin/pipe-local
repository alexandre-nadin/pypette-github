#!/usr/bin/env bash
_dir=$(readlink -f $(dirname "${BASH_SOURCE[0]}"))

## Include binaries
export PATH="${PATH}":"${_dir}"
export WORKFLOW_DIR="${HOME}/dev/ctgb"
export CLUSTER_MNT_POINT="${HOME}/dev/ctgb/cluster"
export TMPDIR='/tmp'
export SHELL_ENV="$HOME/local-usr/.bashrc"
"${_dir}/set-dirs"

pipe "$@"
